{
  "name": "tokenregion-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "pnpm jest -w=1",
    "lint": "next lint",
    "lint:report": "eslint --no-error-on-unmatched-pattern --max-warnings=0",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check:staged": "lint-staged",
    "clean": "rm -rf .next && rm -rf node_modules",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@moralisweb3/common-evm-utils": "^2.22.3",
    "@nextui-org/react": "1.0.0-beta.13",
    "@rainbow-me/rainbowkit": "^1.0.3",
    "moralis": "^2.22.3",
    "next": "^13.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "~1.0.7",
    "wagmi": "~1.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-lodash": "^7.4.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^5.1.3"
  }
}
